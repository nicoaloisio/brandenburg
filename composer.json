{
    "name": "silvanite/brandenburg",
    "description": "Laravel Authentication Package",
    "authors": [
        {
            "name": "Marco Mark (m2de)",
            "email": "marco@silvanite.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silvanite\\Brandenburg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silvanite\\Brandenburg\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silvanite\\Brandenburg\\Providers\\BrandenburgServiceProvider"
            ],
            "aliases": {
                "BrandenburgPolicy": "Silvanite\\Brandenburg\\Facades\\PolicyFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "^3.1",
        "mockery/mockery": "^1.1"
    }
}
